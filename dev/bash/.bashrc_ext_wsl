export NODE_AUTH_TOKEN='<token>';
export GIT_DISCOVERY_ACROSS_FILESYSTEM=1
# windows side home
export HOMES='/mnt/c/Users/<user>/'
export DOTNET_HOST_PATH="`which dotnet`"
